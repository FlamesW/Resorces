-- // This is for the script, It is not meant to be executed.
local CreditsSection1 = getgenv().CreditsTab:Section({Side = "Left"});

CreditsSection1:Header({Text = "Credits Wall"});

CreditsSection1:Divider();

CreditsSection1:Spacer();

CreditsSection1:Paragraph({Header = "Script Creator",Body = "Mommy Flames <3"});

CreditsSection1:Spacer();

CreditsSection1:Paragraph({Header = "Some Sources",Body = "Sirius Team <3"});

CreditsSection1:Divider();

CreditsSection1:SubLabel({Text = "Thanks to all these amazing people <3"});

local CreditsSection2 = getgenv().CreditsTab:Section({Side = "Left"});
CreditsSection2:Header({Text = "Contact me"});

CreditsSection2:Divider();

CreditsSection2:Button({Name = "Get in touch here.",Callback = function()
	Window:Notify({Title = Window.Settings.Title,Description = "Discord link copied to clipboard!",Lifetime = 3});
	setclipboard("https://discord.com/users/656455297979908106");
end})

CreditsSection2:SubLabel({Text = "Please dont contact me for no reason~"});

local CreditsSection3 = getgenv().CreditsTab:Section({Side = "Left"});

CreditsSection3:Button({Name = "Join Discord Server",Callback = function()
	Window:Notify({Title = Window.Settings.Title,Description = "Discord link copied to clipboard!",Lifetime = 3});
	loadstring(game:HttpGet("https://raw.githubusercontent.com/FlamesW/Main/home/Discord"))();
end})

CreditsSection3:SubLabel({Text = "https://dsc.gg/Flames-Community"});

